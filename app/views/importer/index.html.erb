<% form_for :dump, :url=>{:controller=>"importer", :action=>"csv_import"}, :html => { :multipart => true } do |f| -%>

<h4>Upload Users via CSV import...</h4>
<table>
   <tr>
     <td>
      <label for="dump_file">
        Select a CSV File:
      </label>
     </td>
     <td >
       <%= f.file_field :file -%>
     </td>
   </tr>
   <tr><td>
       <label for="dump_file">
        Select project:
      </label>
     </td><td>
       <%=  select("dump", "project_id", Project.find(:all, :order => "id DESC").collect {|p| [ p.title, p.id ] }) %>
     </td>
   </tr>
   <tr>
     <td colspan='2'>
       <%= submit_tag 'Submit' -%>
     </td>
   </tr>
 </table>
<% end -%>